<% if item.object.is_a?(QingGroup) %>
  <% if !item.object.children.empty? %>
    <%= render 'qing_groups/web', qing_group: item %>
    <%= item.fields_for :children do |child| %>
      <%= render 'form_items/web', item: child %>
    <% end %>
  <% end %>
<% elsif item.object.present? && item.object.printable? %>
  <%= render 'questionings/web', qing: item %>
<% end %>
